<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <EditText
        android:id="@+id/editTextLabor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="50dp" />

    <TextView
        android:id="@+id/textViewlabor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/labor"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="63dp" />

    <TextView
        android:id="@+id/textViewSubTotalVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="290dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/calculate"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="213dp" />

    <TextView
        android:id="@+id/textViewSubTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/subtotal"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="290dp" />

    <TextView
        android:id="@+id/textViewTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/total"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/textViewTotalVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="356dp" />

    <TextView
        android:id="@+id/textViewTax"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tax"
        tools:layout_editor_absoluteX="181dp"
        tools:layout_editor_absoluteY="324dp" />

    <TextView
        android:id="@+id/textViewTaxVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="324dp" />

    <EditText
        android:id="@+id/editTextMaterials"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="numberDecimal"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="133dp" />

    <TextView
        android:id="@+id/textViewmaterials"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/materials"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="141dp" />


</androidx.constraintlayout.widget.ConstraintLayout>